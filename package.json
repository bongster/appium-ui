{
  "name": "appium-ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "./node_modules/.bin/mocha --recursive --require test/support/env --check-leaks --bail test/",
    "jshint": "./node_modules/.bin/jshint --verbose .",
    "jscs": "./node_modules/.bin/jscs .",
	"test-ci": "npm run jshint && npm run jscs && npm run test",
	"lint": "npm run jshint && npm run jscs"
  },
  "dependencies": {
    "body-parser": "1.15.1",
    "bootstrap": "^4.0.0-alpha.4",
    "cookie-parser": "1.4.3",
    "debug": "2.2.0",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "express-session": "1.14.1",
    "morgan": "1.7.0",
    "node-sass-middleware": "0.8.0",
    "serve-favicon": "2.3.0"
  },
  "devDependencies": {
    "jscs": "^3.0.7",
    "jshint": "^2.9.3",
    "mocha": "^3.0.2",
    "nodemon": "1.10.2",
    "should": "^11.1.0",
    "sinon": "^1.17.5",
    "supertest": "^2.0.0"
  }
}
